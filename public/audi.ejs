            }]
        },

        series: [{
            name: 'Speed',
            data: [6],
            tooltip: {
                valueSuffix: ' 1/min x 1000'
            }
        }]

    },
        // Add some life
        function (chart) {
            if (!chart.renderer.forExport) {
