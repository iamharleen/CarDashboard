    <img style="margin-bottom:10px; align-self:center" class="active visible-lg img-responsive" src="images/mercedes.jpg" />
    <div class="container" style="width:auto" align="center" role="tabpanel">
	<div class="tabbable">
		<ul class="nav nav-pills nav-justified">
	        <li><a href="#tab0" data-toggle="tab">About</a></li>
        	<li class="active"><a href="/barGraph">2015 Ratings</a></li>
            <li><a href="/piegraph/:mercedes">Sales Ratio</a></li>
            <li><a href="/modelBarGraph/:mercedes">Compare Models</a></li>
            <li><a href="/bubbleGraph">Top Rated Cars</a></li>        
        </ul>
        <div class="tab-content">
        	<div class="tab-pane active">
                <h2>Car Manufacturers Gasoline Comparison for 2015</h2>
                <div id="container" style="min-width: 310px; width: 700px; height: 400px; margin: 0 auto"></div>
            </div><!-- end tab-pane -->
        </div><!-- end tab-content -->
	</div><!---end tabbable -->
