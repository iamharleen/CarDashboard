    <script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/1.8.2/jquery.min.js"></script>
    <script type="text/javascript" src="https://code.highcharts.com/highcharts.js"></script>
    <script type="text/javascript" src="https://code.highcharts.com/modules/exporting.js"></script>   

    <script type="text/javascript">
        $(function () {
            $('#container').highcharts({
                chart: {
                    type: 'column'
                },
                title: {
                    text: 'Stacked column chart'
                },
                xAxis: {
                    categories: ['Audi', 'Mercedes', 'BMW']
                },
                yAxis: {
                    min: 0,
                    title: {
                        text: ''
                    }
                },
                tooltip: {
                    pointFormat: '<span style="color:{series.color}">{series.name}</span>: <b>{point.y}</b> ({point.percentage:.0f}%)<br/>',
                    shared: true
                },
                plotOptions: {
                    column: {
                        stacking: 'percent'
                    }
                },
                series: [{
                    name: 'Premium',
                    data: [<%= output1[0][2].total_products%>, <%= output1[1][3].total_products%>, <%= output1[2][4].total_products%>]
                }, {
                    name: 'Electricity',
                    data: [<%= output1[0][0].total_products%>, <%= output1[1][0].total_products%>, <%= output1[2][2].total_products%>]
                }, {
                    name: 'Diesel',
                    data: [<%= output1[0][1].total_products%>, <%= output1[1][1].total_products%>, <%= output1[2][3].total_products%>]
                }]
            });
        });  
        
    </script>
        <!-- Footer -->
        <footer class="footer-basic-centered">
            <p class="footer-company-motto">Work hard, play even harder</p>
            <p class="footer-company-name">Elite Technologies &copy; 2015</p>
        </footer>
