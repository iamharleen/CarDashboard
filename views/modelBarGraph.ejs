    <script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/1.8.2/jquery.min.js"></script>
    <script type="text/javascript" src="https://code.highcharts.com/highcharts.js"></script>
    <script type="text/javascript" src="https://code.highcharts.com/modules/exporting.js"></script>   


    <script type="text/javascript">
    $(function () {
        $('#container').highcharts({
            chart: {
                type: 'column'
            },
            title: {
                text: 'Number of Cylinders'
            },
            subtitle: {
                text: ''
            },
            xAxis: {
                categories: ['<%= output1[0]._id.model%>',
                             '<%= output1[1]._id.model%>',
                             '<%= output1[2]._id.model%>',
                             '<%= output1[3]._id.model%>',
                             '<%= output1[4]._id.model%>',
                             '<%= output1[5]._id.model%>',
                             '<%= output1[6]._id.model%>',
                             '<%= output1[7]._id.model%>',
                             '<%= output1[8]._id.model%>',
                             '<%= output1[9]._id.model%>',
                             '<%= output1[10]._id.model%>'
                            ],
                crosshair: true
            },
            yAxis: {
                min: 0,
                title: {
                    text: 'Cylinder'
                }
            },
            tooltip: {
                headerFormat: '<span style="font-size:10px">{point.key}</span><table>',
                pointFormat: '<tr><td style="color:{series.color};padding:0">{series.name}: </td>' +
                    '<td style="padding:0"><b>{point.y:.1f} mm</b></td></tr>',
                footerFormat: '</table>',
                shared: true,
                useHTML: true
            },
            plotOptions: {
                column: {
                    pointPadding: 0.2,
                    borderWidth: 0
                }
            },
            series: [{
                name: 'Mercedes',
                data: [<%= output1[0]._id.cylinders%>, <%= output1[1]._id.cylinders%>,<%= output1[2]._id.cylinders%>,<%= output1[3]._id.cylinders%>,
                       <%= output1[4]._id.cylinders%>,<%= output1[5]._id.cylinders%>,<%= output1[6]._id.cylinders%>,<%= output1[7]._id.cylinders%>,
                       <%= output1[8]._id.cylinders%>,<%= output1[9]._id.cylinders%>,<%= output1[10]._id.cylinders%>]

            }]
        });
    });
        
    </script>
    <!-- Footer -->
        <footer class="footer-basic-centered">

            <p class="footer-company-motto">Work hard, play even harder</p>
            <p class="footer-company-name">Elite Technologies &copy; 2015</p>

        </footer>
</body>
